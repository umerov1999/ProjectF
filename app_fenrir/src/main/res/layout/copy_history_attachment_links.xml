<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <dev.ragnarok.fenrir.view.mozaik.MozaikLayout
        android:id="@+id/copy_history_photo_attachments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <dev.ragnarok.fenrir.view.AudioContainer
        android:id="@+id/copy_history_audio_attachments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingStart="16dp"
        android:paddingTop="4dp"
        android:paddingEnd="16dp"
        android:paddingBottom="4dp" />

    <LinearLayout
        android:id="@+id/copy_history_video_attachments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingStart="16dp"
        android:paddingTop="4dp"
        android:paddingEnd="16dp"
        android:paddingBottom="4dp" />

    <LinearLayout
        android:id="@+id/copy_history_stickers_attachments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="-4dp" />

    <LinearLayout
        android:id="@+id/copy_history_docs_attachments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingStart="16dp"
        android:paddingTop="4dp"
        android:paddingEnd="16dp"
        android:paddingBottom="4dp" />

    <LinearLayout
        android:id="@+id/copy_history_articles_attachments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingStart="16dp"
        android:paddingTop="4dp"
        android:paddingEnd="16dp"
        android:paddingBottom="4dp" />
</merge>