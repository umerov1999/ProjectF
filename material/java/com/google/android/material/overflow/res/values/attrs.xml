<resources>
  <!-- Style to use for OverflowLayoutStyle in this theme. -->
  <attr name="overflowLinearLayoutStyle" format="reference"/>

  <!-- Style for the overflow button of an overflow layout in this theme. -->
  <attr name="overflowLinearLayoutOverflowButtonStyle" format="reference"/>
  <!-- Style for the overflow menu of an overflow layout in this theme. -->
  <attr name="overflowLinearLayoutPopupMenuStyle" format="reference"/>

  <declare-styleable name="OverflowLinearLayout">
    <!-- The icon of the overflow button of an overflow layout. -->
    <attr name="overflowButtonIcon"/>
  </declare-styleable>

  <declare-styleable name="OverflowLinearLayout_Layout">
    <!-- The icon of an item inside an overflow layout's overflow menu. -->
    <attr name="layout_overflowIcon"/>
    <!-- The text of an item inside an overflow layout's overflow menu. -->
    <attr name="layout_overflowText"/>
  </declare-styleable>
</resources>
